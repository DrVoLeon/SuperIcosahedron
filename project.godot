; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SuperIcosahedron"
run/main_scene="res://v2/scenes/MainScene.tscn"
run/disable_stdout=true
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/image="res://icon.png"
boot_splash/fullsize=false
config/icon="res://icon.svg"
config/windows_native_icon="res://icon.ico"

[autoload]

G="*res://v2/globals.gd"

[debug]

settings/stdout/print_fps=true
gdscript/warnings/unassigned_variable=0
gdscript/warnings/unused_variable=0
gdscript/warnings/unused_local_constant=0
gdscript/warnings/unused_parameter=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="canvas_items"

[filesystem]

import/blender/enabled=false

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
renderer/rendering_method.web="mobile"
textures/vram_compression/import_etc2_astc=true
